    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch SampleLauncher",
            "request": "launch",
            "mainClass": "seocrawler.SampleLauncher",
            "projectName": "seocrawler"
        },
        {
            "name": "Listen for XDebug",
            "type": "php",
            "request": "launch",
            "port": 9000
        },
        {
            "log": true,
            "name": "XDebug listening to Laradock",
            "type": "php",
            "request": "launch",
            "port": 9000,
            "pathMappings": {
                "/var/www/": "/media/muhammad/disk/projects/seo-saas/"
            }
        },
        {
            "name": "Launch currently open script",
            "type": "php",
            "request": "launch",
            "program": "${file}",
            "cwd": "${fileDirname}",
            "port": 9000
        }
    ]
}